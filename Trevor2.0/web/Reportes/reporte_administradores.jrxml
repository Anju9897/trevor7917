	<parameter name="fin" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select 
	u.nombres as 'Nombres',
	u.apellidos as 'Apellidos',
	e.estado as 'Estado',
	count(t.idestado) as '# Tickets',
	convert(varchar(3),t.fecha_emision,100) as 'Mes'
from ticket t 
	inner join usuario u on (t.u_encargado = u.idusuario) 
	inner join estado e on (t.idestado = e.idestado) 
group by u.nombres,u.apellidos,e.estado,t.fecha_emision having (t.fecha_emision between $P{inicio} and $P{fin})
order by u.nombres;]]>
	</queryString>
