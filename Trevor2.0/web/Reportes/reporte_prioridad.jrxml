	<queryString>
		<![CDATA[select 
	ht.idticket as 'id Ticket',
	t.fecha_emision as 'Fecha Emision',
	t.Tipo as 'Tipo de Problema',
	t.prioridad as 'Prioridad',
	e.estado as 'Estado',
	 isnull(convert(varchar(10),ht.fecha_final,105),'---------') as 'Fecha Finalizacion'
from historialTicket ht 
	inner join ticket t on (ht.idticket = t.idticket) 
	inner join estado e on (t.idestado = e.idestado)
where (t.fecha_emision between $P{inicio} and $P{fin})
order by t.prioridad;]]>
	</queryString>
